<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
	<settings defaultResolver="chained" />
	<credentials host="webdev.juliasoft.com" realm="Repository Archiva Managed julia-private Repository" username="${julia.archiva.usr}" passwd="${julia.archiva.pass}" />
	<resolvers>
		<!-- try to resolve dependency using the first repo in list providing the 
			needed artifacts -->
		<chain name="chained" returnFirst="true">
			<ibiblio name="julia-private" m2compatible="true" 
				root="https://webdev.juliasoft.com/apache-archiva-2.0.1/repository/julia-private" pattern="[module]/[artifact]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
			<ibiblio name="maven2" m2compatible="true" />
		</chain>
	</resolvers>
</ivysettings>